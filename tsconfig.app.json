{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./*.d.ts",
    "src/**/*",
    "src/**/*.vue",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@panel/*": [
        "./src/panel/*"
      ],
      "@notice/*": [
        "./src/notice/*"
      ]
    },   
  },
}